{
  "manifest_version": 3,
  "name": "semantic-bookmarker",
  "version": "1.0.0",
  "description": "DEVELOPMENT build with simple auto-reloader\n[2024-04-18 23:02:56] waiting for changes...",
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "cookies",
    "tabs"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "assets/icons/16.png",
      "24": "assets/icons/24.png",
      "32": "assets/icons/32.png"
    },
    "default_title": "Semantic Bookmarker",
    "default_popup": "pages/popup/index.html"
  },
  "icons": {
    "48": "assets/icons/64.png",
    "64": "assets/icons/64.png",
    "128": "assets/icons/128.png",
    "256": "assets/icons/256.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons/*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}